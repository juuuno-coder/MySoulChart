# Google Gemini API Key
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Firebase Configuration
# Get from Firebase Console > Project Settings > General > Your apps > SDK setup and configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef1234567890

# Kakao JavaScript Key
# Get from: https://developers.kakao.com/console/app
VITE_KAKAO_APP_KEY=your_kakao_javascript_key

# Google Analytics 4 Measurement ID (Optional)
# Get from: https://analytics.google.com/
# Note: This should be set directly in index.html
# VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
